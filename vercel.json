{
  "version": 2,
  "buildCommand": "npm run vercel-build",
  "outputDirectory": "dist/browser",
  "installCommand": "npm install",
  "framework": "angular",
  "rewrites": [
    { "source": "/remoteEntry.js", "destination": "/remoteEntry.js" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}