{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "buildCommand": "npm run build",
        "outputDirectory": "dist"
      }
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/remoteEntry.js",
      "dest": "/browser/remoteEntry.js"
    },
    {
      "src": "/(.*)",
      "dest": "/browser/$1"
    }
  ]
}