{
  "version": 2,
  "builds": [
    {
      "src": "dist/mfe1/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/remoteEntry.js",
      "dest": "/remoteEntry.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}